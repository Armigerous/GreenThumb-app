{
  "expo": {
    "name": "theGreenThumb-app",
    "slug": "theGreenThumb-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "TheGreenThumb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-updates",
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you add images of your plants.",
          "cameraPermission": "The app needs access to your camera to let you take photos of your plants."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e0e1a7e6-9d15-4a3c-92c3-85401a72814d"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e0e1a7e6-9d15-4a3c-92c3-85401a72814d"
      }
    },
    "owner": "armigerous"
  }
}
